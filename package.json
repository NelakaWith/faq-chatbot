{
  "name": "faq-chatbot-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "dev/backend",
    "dev/frontend"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "release": "changeset version && git add . && git commit -m \"chore(release): version packages\" && git push --follow-tags",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@changesets/changelog-github": "^0.5.0"
  }
}
